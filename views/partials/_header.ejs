<h1>TinyApp Project</h1>
<form action="/login" method="POST">
  <label for="username">Enter Username:</label>
  <input id="username" name="username">
  <button>Sign In</button>
</form>

<% if (username) { %>
  <%= `${username}` %>
  <form action="/logout" method="POST">
    <button>Sign Out</button>
  </form>
<% } %>


